package com.inventory.myexception;

import java.util.HashMap;

public class ExcelValidationExceptionHandling extends RuntimeException {

	private static final long serialVersionUID = 1L;

	private HashMap<String, String> errorMessages;

	public HashMap<String, String> getErrorMessages() {
		return errorMessages;
	}

	public void setErrorMessages(HashMap<String, String> errorMessages) {
		this.errorMessages = errorMessages;
	}

	public ExcelValidationExceptionHandling(HashMap<String, String> errorMessages) {
		super();
		this.errorMessages = errorMessages;
	}

}
